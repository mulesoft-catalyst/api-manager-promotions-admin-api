#%RAML 1.0 DataType
type: object
properties:
  id:
    type: number
    description: The id of the app
  applicationName:
    type: string
    description: The application name that was created
  credentials:
    type: object
    required: false
    properties:    
      clientId:
        type: string
        description: The clientId of the app
      clientSecret: 
        type: string
        description: The clientSecret of the app
    